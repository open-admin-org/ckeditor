{
    "name": "open-admin-ext/ckeditor",
    "description": "Integrate CKEDITOR into open-admin",
    "type": "library",
    "keywords": ["open-admin", "wysiwyg", "extension", "editor", "CKEditor"],
    "homepage": "https://github.com/open-admin-ext/ckeditor",
    "license": "MIT",
    "authors": [
        {
            "name": "open-admin",
            "email": "info@open-admin.org"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=7.0",
        "open-admin-org/open-admin": "*",
        "ckeditor/ckeditor": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "laravel/laravel": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "OpenAdmin\\Admin\\CKEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenAdmin\\Admin\\CKEditor\\CKEditorServiceProvider"
            ]
        }
    }
}
